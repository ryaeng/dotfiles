[user]
	name = Ryan C. England 
	email = rcengland@gmail.com
[core]
 	editor = nvim
 	excludesfile = /Users/ryan/.gitignore
	pager = delta
[interactive]
	diffFilter = delta --color-only
[color]
	ui = true
[diff]
	colorMoved = default
[delta]
	navigate = true
	line-numbers = true
[merge]
	conflictstyle = diff3
